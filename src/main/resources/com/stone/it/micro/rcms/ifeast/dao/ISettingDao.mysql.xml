<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.stone.it.micro.rcms.ifeast.dao.ISettingDao">

  <select id="getSettingList" resultType="com.stone.it.micro.rcms.ifeast.vo.SettingVO">
    <include refid="query_base"></include>
  </select>
  <select id="getSettingById" resultType="com.stone.it.micro.rcms.ifeast.vo.SettingVO">
    <include refid="query_base"></include>
    where setting_id = #{settingId}
  </select>
  <sql id="query_base">
    select
    setting_id as settingId,
    type as type,
    domain,
    port,
    root_path as rootPath,
    user_account as account,
    password
    from tpl_mv_setting_t
  </sql>

</mapper>